{
  "extends": ["eslint:recommended", "plugin:vue/vue3-recommended"],
  "rules": {
    "vue/component-name-in-template-casing": ["error", "PascalCase"]
  },
  "plugins": ["jest", "cypress"],
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true,
    "cypress/globals": true
  }
}
